{
    "plugins": ["prettier-plugin-go-template"],
    "trailingComma": "none",
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 140,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.md",
            "options": {
                "printWidth": 200
            }
        },
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template"
            }
        }
    ]
}
